// DIE's signature file

init("library","QT");

function detect(bShowType,bShowVersion,bShowOptions)
{
    if(ELF.isStringInTablePresent(".dynstr","/usr/local/Trolltech/Qt-4.8.5/lib"))
    {
        sVersion="4.8.5";
        bDetected=1;
    }
    else if(ELF.isStringInTablePresent(".dynstr","libQtCore.so.4"))
    {
        sVersion="4.X";
        bDetected=1;
    }

    return result(bShowType,bShowVersion,bShowOptions);
}
