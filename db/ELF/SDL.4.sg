// DIE's signature file

init("library","SDL");

function detect(bShowType,bShowVersion,bShowOptions)
{
    if(ELF.isStringInTablePresent(".dynstr","libSDL-1.2.so.0"))
    {
        sVersion="1.2";
        bDetected=1;
    }
    else if(ELF.isStringInTablePresent(".dynstr","SDL_Init"))
    {
        bDetected=1;
    }

    return result(bShowType,bShowVersion,bShowOptions);
}
