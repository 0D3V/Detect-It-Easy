init("compiler","Free Pascal");

function detect(bShowType,bShowVersion,bShowOptions)
{
    if(ELF.compareEP("31ED5989E3"))
    {
        bDetected=1;
    }
    else if(ELF.isSectionNamePresent(".fpc.resources"))
    {
        bDetected=1;
    }

    return result(bShowType,bShowVersion,bShowOptions);
}
