init("protector","Gardian Angel");

function detect(bShowType,bShowVersion,bShowOptions)
{
    if(MSDOS.compareEP("068cc88ed88ec0fcbf....8be8e4..0c..eb"))
    {
        sVersion="1.0 EXE";
        sOptions="by Stefan Verkoyen";
        bDetected=1;
    }    
    else if(MSDOS.compareEP("068CC88ED88EC0FCBF....EB"))
    {
        sVersion="1.0";
        bDetected=1;
    }

    return result(bShowType,bShowVersion,bShowOptions);
}
