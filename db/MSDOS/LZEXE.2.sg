// DIE's signature file

init("packer","LZEXE");

function detect(bShowType,bShowVersion,bShowOptions)
{
    if(MSDOS.compare("'LZ09'",0x1c))
    {
        sVersion="0.90";
        bDetected=1;
    }
    else if(MSDOS.compare("'LZ91'",0x1c))
    {
        sVersion="0.91";
        bDetected=1;
    }

    return result(bShowType,bShowVersion,bShowOptions);
}
