        sName = "DreamStation II module (.DS2)";
        sVersion = "v2";
        if (Binary.isVerbose()) {
            ts=Binary.read_uint8(0x62); t=Binary.getString(0x63,ts);
            pa=0x62+ts+1; as=Binary.read_uint8(pa); a=Binary.getString(pa+1,as);
            pc=pa+as+1;  cs=Binary.read_uint8(pc); c=Binary.getString(pc+1,cs);
            sOption(t);
            sOption(a,"by: ");
