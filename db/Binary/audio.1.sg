    else if(Binary.compare("'MMU2'")) {
        bDetected = 1;
        sName = "Digital Sound Studio module (.DSS)";
        if (Binary.isVerbose())
        sOptionT(Binary.getString(10,32));
    }
    else if(Binary.compare("'D.T.'00")) {
        sv = ["S.Q.","VERS"].indexOf(Binary.getString(0x2A,4));
        if(sv >= 0) {
            bDetected = 1;
            sName = "Digital Tracker module (.DTM)";
            if (Binary.isVerbose()) {
                if(sv==0) sOptionT(Binary.getString(0x16,20))
                //let's play along with Bummtschak here
                else sOptionT(Binary.getString(0x16,24))
            }
