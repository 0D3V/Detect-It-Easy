init("installer","Tarma Installer");

function detect(bShowType,bShowVersion,bShowOptions)
{
    if(/Tarma Installer/.test(PE.getManifest()))
    {
        sVersion=PE.getFileVersion();
        bDetected=1;
    }

    return result(bShowType,bShowVersion,bShowOptions);
}
