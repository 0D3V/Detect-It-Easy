// DIE's signature file

init("protector","CronosCrypter");

function detect(bShowType,bShowVersion,bShowOptions)
{
    if(PE.isNET())
    {
		if(PE.isNETStringPresent("Reserved1") && PE.isNETStringPresent("Reserved2") && PE.isNETStringPresent(".ctor") && PE.isNETStringPresent("RijndaelManaged") && PE.isNETStringPresent("CipherMode")){
			bDetected=1;
		}
    }

    return result(bShowType,bShowVersion,bShowOptions);
}
