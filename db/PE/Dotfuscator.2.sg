init("protector","Dotfuscator");

function detect(bShowType,bShowVersion,bShowOptions)
{
    if(PE.isNETStringPresent("DotfuscatorAttribute"))
    {
        if (PE.isHeuristicScan()) {
            bDetected=1;
        }
