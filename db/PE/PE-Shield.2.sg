init("protector","PE-SHiELD");

function detect(bShowType,bShowVersion,bShowOptions)
{
    var nOffset=0;
    sVersion="0.26";
    if(PE.compareEP("E9"))
    {
        sVersion="0.25";
        nOffset=~~PE.readDword(PE.nEP+1)+5;
    }
    if(PE.compareEP("60e8$$$$$$$$5d83ed..eb$$8db5........ba........8a..1632..80....80....88....4aeb",nOffset))
    {
        bDetected=1;
    }
    
    return result(bShowType,bShowVersion,bShowOptions);
}
