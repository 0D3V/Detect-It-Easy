// DIE's signature file

init("protector","DeepSea");

function detect(bShowType,bShowVersion,bShowOptions)
{
    if(PE.isNET())
    {
        var nOffset=PE.findString(PE.getSectionFileOffset(0),
                                  PE.getSectionFileSize(0),
                                  "DeepSea Obfuscator");
        if(nOffset!=-1)
        {
            if(PE.compare("'Evaluation'",nOffset+19))
            {
                sOptions="Evaluation";
            }
            bDetected=1;
        }
    }

    return result(bShowType,bShowVersion,bShowOptions);
}
